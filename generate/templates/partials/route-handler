export async function {{METHOD_NAME}}(request: NextRequest): Promise<NextResponse> {
    try {
        const data: any = await request.json()

        return NextResponse.json({ data }, { status: SuccessStatus.OK })
    } catch (error: unknown) {
        let status: FailureStatus

        if (error instanceof CustomError && error.status) {
            status = error.status
        } else {
            status = FailureStatus.BAD_REQUEST
        }

        return NextResponse.json({ error }, { status })
    }
}